){\n      \n       t <- seq(1:length(datosTS))                     \n       m <- lm(formula = datosTS ~ t)\n       \n       plot(t,datosTS, type = \"l\")\n       lines(m$fitted.values, col = \"red\", lwd = 2)\n       #plot(datosTS)\n     }else{\n       if(input$radio2==2){\n   \n         \n         \n       }else{\n         if(input$radio2==3){\n           \n           \n           \n         }\n       }\n     }   \n   })  \n \n  \n  \n\n})\n",
